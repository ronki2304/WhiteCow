        <add key="BitFinex.NbCallPost" value="240"/>
        <add key="BitFinex.PostInterval" value="60"/>
         <add key="BitFinex.key" value="xxx"/>
        <add key="BitFinex.secret" value="xxx"/>
        <add key="BitFinex.QuoteCurrency" value="LTC,ETH,ETC,RRT,ZEC,XMR,DASH,XRP,IOTA,EOS,SAN,OMG,BCH,NEO,ETP,QTUM,AVT,EDO,BTG,DATA,QASH,YYW,GNT,SNT,BAT,MNA,FUN,ZRX,TNB,SPK,TRX,RCN,RLC,AID,SNG,REP,ELF,AION,IOST,REQ,RDN,LRC,BFT,CFI,WAX,AGI,MTN,ODEM,DAI"/> 
        <add key="BitFinex.MinimumSize" value="0.01"/>

        <add key="Poloniex.geturl" value="https://poloniex.com/"/>
        <add key="Poloniex.posturl" value="https://poloniex.com/tradingApi"/>
        <add key="Poloniex.NbCallPost" value="6"/>
