        <add key="Poloniex.key" value="xxx"/>
        <add key="Poloniex.secret" value="xxx"/>
        <add key="Poloniex.QuoteCurrency" value="ETH,XRP,XMR,LTC,STR,DASH,DOGE,BTS,MAID,FCT,CLAM" />
